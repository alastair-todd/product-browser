{
  "name": "product-gallery",
  "version": "1.0.0",
  "description": "A simple product browser",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "browserify -t [ babelify --presets [ es2015 react ] ] src/index.js --debug -o build/app.js -v",
    "watch": "watchify -t [ babelify --presets [ es2015 react ] ] src/index.js -o build/app.js -v",
    "build": "browserify -t [ babelify --presets [ react ] ] src/index.js -o build/app.js",
    "build-sass": "node-sass assets/sass/app.scss assets/css/app.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alastair-todd/product-browser.git"
  },
  "author": "Alastair Todd",
  "license": "ISC",
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "ecstatic": "^2.1.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "standard": "^7.1.2",
    "watchify": "^3.7.0"
  }
}
